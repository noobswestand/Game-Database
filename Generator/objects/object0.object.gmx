repeat(150){
    var fname=name(3,12);
    var lname=name(5,12);
    var address=string(irandom_range(100,1000))+" "+name(5,10)+" "+choose("Ave","St","Way","Lane");
    
    var pets=irandom_range(1,4);
     
    repeat(pets){
         var pname=name(3,10);
         var date=string(irandom_range(1990,2017))+"-"+string(irandom(11)+1)+"-"+string(irandom(27)+1);
         file_text_write_string(file,'INSERT INTO pets(PetID,Name,DateOfBirth) VALUES(null,'
         +'"'+pname+'",'+"DATE '"+date+"');")
         file_text_writeln(file)
         petCount++
     }
     
     file_text_write_string(file,'INSERT INTO owner(OwnerID,FirstName,LastName,Address) VALUES(null,'
     +'"'+fname+'",'+'"'+lname+'",'+'"'+address+'");')
     file_text_writeln(file)
     ownerCount++
     
     var pets2=petCount-pets+1;
     repeat(pets){
        file_text_write_string(file,'INSERT INTO petowner(OwnerID,PetID) VALUES('+string(ownerCount)+','+string(pets2)+');')
        file_text_writeln(file)
        pets2++
     }
     
     
     var app=irandom(5);
     repeat(app){
        var date2=string(irandom_range(2000,2017))+"-"+string(irandom(11)+1)+"-"+string(irandom(27)+1);
        file_text_write_string(file,'INSERT INTO appointments(AppointmentID,DateofApp,PetID) VALUES(null,'
        +"DATE '"+date2+"',"+string(irandom_range(petCount-pets+1,petCount))+");")
        file_text_writeln(file)
     }
     
     
    
}

file_text_close(file)

