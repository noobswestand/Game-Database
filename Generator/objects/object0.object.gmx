//Tags - 338 tags
if !file_exists("save.sql"){
    scr_tags();
}


file=file_text_open_append("save.sql")
var userAmount=500;//irandom_range(2,15);
userCount+=userAmount
repeat(userAmount){
    //Users
    var fname=name(3,12);
    var lname=name(5,12);
    var year=irandom_range(1960,2013)
    var date=string(year)+"-"+string(irandom(11)+1)+"-"+string(irandom(27)+1);
    var date1=string(irandom_range(year+1,2013))+"-"+string(irandom(11)+1)+"-"+string(irandom(27)+1);
    var gender=choose('"F"','"M"');
    var img='"'+image(256,"profile")+'"';
    var pass=sha1_string_unicode("password12345")
    var _salt=rnd_string(128);
    pass=sha1_string_unicode(pass+_salt)
    file_text_write_string(file,"INSERT INTO users(UserID,FirstName,LastName,DateOfBirth,Gender,ProfilePicture,Salt,HashPass,RegDate) VALUES(null,"+
        +'"'+fname+'",'+'"'+lname+'",'+"DATE '"+date+"',"+gender+","+img+',"'+_salt+'",'+'"'+pass+'", '+"DATE '"+date1+"');")
    file_text_writeln(file)
}
//Friends
for(var i=userCount-userAmount;i&lt;userCount;i++){
    for(var a=userCount-userAmount;a&lt;userCount;a++){
        if a!=i{//can't be own friend
            
            if irandom(abs(a-i))==0{
                file_text_write_string(file,"INSERT INTO friends(UserID,FriendID) VALUES("+string(i)+","+string(a)+");")
                file_text_writeln(file)
            }
            
        }
    }
}

//Games
gameAmount=500

repeat(gameAmount){
    var gameName=name(3,15)
    var gameDes=sentence(5,15)
    var gamePrice=irandom(60)+choose(0,.99);
    var gameRate=choose("E","eC","E10+","T","M","A")
    var gamePublisher=name(5,15);
    var gameDeveloper=choose(name(5,15),gamePublisher)
    var gameRelease=string(irandom_range(1960,2013))+"-"+string(irandom(11)+1)+"-"+string(irandom(27)+1);
    
     file_text_write_string(file,"INSERT INTO games(GameID,Price,Name,Description,Rating,Publisher,Developer,ReleaseDate) VALUES(null,"+
     string(gamePrice)+","+'"'+gameName+'",'+'"'+gameDes+'",'+'"'+gameRate+'",'+'"'+gamePublisher+'",'+'"'+gameDeveloper+'"'+",DATE '"+gameRelease+"');")
     file_text_writeln(file)
     
     //DLC
     var dlc=irandom(5);
     repeat(dlc){
        var dlcPrice=round(random(30))+random_range(0,.99);
        var dlcName='"'+name(5,15)+'"'
        var dlcDes='"'+sentence(5,15)+'"'
        file_text_write_string(file,"INSERT INTO dlc(DLCID,GameID,Price,Name,Description) VALUES(null,"+
            string(gameCount)+","+string(dlcPrice)+","+string(dlcName)+","+dlcDes+");")
        file_text_writeln(file)
     }
     
     //Tag the game
     var tagAmount=irandom_range(3,7);
     repeat(tagAmount){
         file_text_write_string(file,"INSERT INTO gametag(GameID,TagID) VALUES("+string(gameCount)+","+string(irandom(337)+1)+");")
         file_text_writeln(file)
     }
    gameCount++
}
//GamePictures
for(var a=gameCount-gameAmount;a&lt;gameCount;a++){
    
    repeat(irandom_range(2,9)){
        var img='"'+image(1024,"game",irandom_range(50,75))+'"';
        file_text_write_string(file,"INSERT INTO gamepictures(PictureID,GameID,Picture) VALUES(null,"+string(a)+","+img+");")
        file_text_writeln(file)
        
    }
}




//gamesOwned
for(var i=userCount-userAmount;i&lt;userCount;i++){//users
    for(var a=gameCount-gameAmount;a&lt;gameCount;a++){//games
        
        if irandom(15)==0{
            var date=string(irandom_range(1960,2013))+"-"+string(irandom(11)+1)+"-"+string(irandom(27)+1);
            var comments=choose("null","null",'"'+sentence(5,25)+'"')
            var hours=irandom(1000)
            var rate;
            if comments=="null"{
                rate=choose("null","null",string(irandom_range(1,5)));
            }else{
                rate=string(irandom_range(1,5))
            }
            file_text_write_string(file,"INSERT INTO gamesowned(UserID,GameID,BoughtDate,Comments,HoursPlayed,Rating) VALUES("+
                string(i)+","+string(a)+','+"DATE '"+date+"',"+comments+","+string(hours)+","+rate+");")
            file_text_writeln(file)
        }
        
    }
}

file_text_close(file)
